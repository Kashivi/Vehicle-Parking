\n  width: 80px;\r\n  height: 80px;\r\n  filter: drop-shadow(0 4px 12px var(--shadow));\r\n}\r\n\r\n@keyframes float {\r\n  0%, 100% { transform: translateY(0px); }\r\n  50% { transform: translateY(-10px); }\r\n}\r\n\r\n.main-title {\r\n  font-size: 3.5em;\r\n  font-weight: 800;\r\n  margin-bottom: 10px;\r\n  letter-spacing: -2px;\r\n}\r\n\r\n.gradient-text {\r\n  background: linear-gradient(135deg, var(--primary-blue), var(--secondary-blue), var(--accent-orange));\r\n  -webkit-background-clip: text;\r\n  -webkit-text-fill-color: transparent;\r\n  background-clip: text;\r\n  animation: shimmer 3s ease-in-out infinite;\r\n}\r\n\r\n@keyframes shimmer {\r\n  0%, 100% { background-position: 0% 50%; }\r\n  50% { background-position: 100% 50%; }\r\n}\r\n\r\n.subtitle {\r\n  font-size: 1.3em;\r\n  color: var(--medium-gray);\r\n  font-weight: 600;\r\n  margin-bottom: 15px;\r\n  text-transform: uppercase;\r\n  letter-spacing: 1px;\r\n}\r\n\r\n.description {\r\n  font-size: 1.1em;\r\n  color: var(--medium-gray);\r\n  line-height: 1.6;\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n}\r\n\r\n/* Features Grid */\r\n.features-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));\r\n  gap: 20px;\r\n  margin-bottom: 50px;\r\n}\r\n\r\n.feature-card {\r\n  background: linear-gradient(145deg, var(--white), var(--light-gray));\r\n  padding: 30px 20px;\r\n  border-radius: 16px;\r\n  text-align: center;\r\n  box-shadow: \r\n    0 8px 32px var(--shadow),\r\n    inset 0 1px 0 rgba(255, 255, 255, 0.9);\r\n  border: 1px solid rgba(255, 255, 255, 0.3);\r\n  transition: all 0.3s ease;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n\r\n.feature-card::before {\r\n  content: '';\r\n  position: absolute;\r\n  top: 0;\r\n  left: -100%;\r\n  width: 100%;\r\n  height: 100%;\r\n  background: linear-gradient(90deg, transparent, rgba(59, 130, 246, 0.1), transparent);\r\n  transition: left 0.5s ease;\r\n}\r\n\r\n.feature-card:hover {\r\n  transform: translateY(-5px);\r\n  box-shadow: \r\n    0 12px 48px var(--shadow-dark),\r\n    inset 0 1px 0 rgba(255, 255, 255, 0.9);\r\n}\r\n\r\n.feature-card:hover::before {\r\n  left: 100%;\r\n}\r\n\r\n.feature-icon {\r\n  font-size: 2.5em;\r\n  margin-bottom: 15px;\r\n  display: block;\r\n}\r\n\r\n.feature-card h3 {\r\n  font-size: 1.2em;\r\n  font-weight: 700;\r\n  color: var(--dark-gray);\r\n  margin-bottom: 8px;\r\n}\r\n\r\n.feature-card p {\r\n  color: var(--medium-gray);\r\n  font-size: 0.95em;\r\n}\r\n\r\n/* Action Buttons */\r\n.action-section {\r\n  display: flex;\r\n  justify-content: center;\r\n  gap: 20px;\r\n  margin-bottom: 50px;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n.btn {\r\n  display: inline-flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  padding: 16px 32px;\r\n  font-size: 1.1em;\r\n  font-weight: 600;\r\n  text-decoration: none;\r\n  border-radius: 12px;\r\n  transition: all 0.3s ease;\r\n  border: none;\r\n  cursor: pointer;\r\n  min-width: 160px;\r\n  position: relative;\r\n  overflow: hidden;\r\n  text-transform: uppercase;\r\n  letter-spacing: 0.5px;\r\n}\r\n\r\n.btn-content {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 8px;\r\n  position: relative;\r\n  z-index: 2;\r\n}\r\n\r\n.btn-icon {\r\n  width: 20px;\r\n  height: 20px;\r\n}\r\n\r\n.btn-primary {\r\n  background: linear-gradient(135deg, var(--primary-blue), var(--secondary-blue));\r\n  color: var(--white);\r\n  box-shadow: 0 8px 32px rgba(30, 64, 175, 0.3);\r\n}\r\n\r\n.btn-primary::before {\r\n  content: '';\r\n  position: absolute;\r\n  top: 0;\r\n  left: -100%;\r\n  width: 100%;\r\n  height: 100%;\r\n  background: linear-gradient(135deg, var(--secondary-blue), var(--accent-orange));\r\n  transition: left 0.3s ease;\r\n  z-index: 1;\r\n}\r\n\r\n.btn-primary:hover {\r\n  transform: translateY(-2px);\r\n  box-shadow: 0 12px 48px rgba(30, 64, 175, 0.4);\r\n}\r\n\r\n.btn-primary:hover::before {\r\n  left: 0;\r\n}\r\n\r\n.btn-secondary {\r\n  background: var(--white);\r\n  color: